{
  "name": "spellchecker-addin-office",
  "version": "1.3.0",
  "scripts": {
    "build": "ng build --configuration production --base-href=/addin/",
    "build:staging": "ng build --configuration test  --base-href=/addin/",
    "build:dev": "ng build",
    "dev-server": "bash -c \"ng serve --ssl --ssl-key ~/.office-addin-dev-certs/localhost.key --ssl-cert ~/.office-addin-dev-certs/localhost.crt\"",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "ng": "ng",
    "ssl:config": "office-addin-dev-certs install",
    "start:desktop": "office-addin-debugging start ./src/manifest.xml desktop",
    "start:desktop:prod": "office-addin-debugging start ./src/manifest-prod.xml desktop",
    "start:web": "office-addin-debugging start ./src/manifest.xml web",
    "start:web:prod": "office-addin-debugging start ./src/manifest-prod.xml web",
    "stop": "office-addin-debugging stop ./src/manifest.xml",
    "stop:prod": "office-addin-debugging stop ./src/manifest-prod.xml",
    "test": "ng test",
    "test:coverage": "ng test --watch false --code-coverage",
    "validate": "office-addin-manifest validate ./src/manifest.xml",
    "validate:staging": "office-addin-manifest validate ./src/manifest-staging.xml",
    "validate:prod": "office-addin-manifest validate ./src/manifest-prod.xml"
  },
  "license": "MIT",
  "config": {
    "app-to-debug": "word",
    "app-type-to-debug": "desktop",
    "dev-server-port": 4200
  },
  "dependencies": {
    "@angular/animations": "^19.1.6",
    "@angular/common": "^19.1.6",
    "@angular/compiler": "^19.1.6",
    "@angular/core": "^19.1.6",
    "@angular/forms": "^19.1.6",
    "@angular/platform-browser": "^19.1.6",
    "@angular/platform-browser-dynamic": "^19.1.6",
    "@angular/router": "^19.1.6",
    "@farscrl/rumantsch-language-tools": "^1.0.3",
    "@iharbeck/ngx-virtual-scroller": "^19.0.1",
    "@microsoft/office-js-helpers": "^1.0.2",
    "@ngneat/dialog": "^5.1.2",
    "core-js": "^3.40.0",
    "ngx-matomo-client": "^7.0.1",
    "ngx-spinner": "^19.0.0",
    "ngx-toastr": "^19.0.0",
    "office-ui-fabric-js": "^1.5.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1901.7",
    "@angular-devkit/build-angular": "^19.1.7",
    "@angular-eslint/builder": "19.1.0",
    "@angular-eslint/eslint-plugin": "19.1.0",
    "@angular-eslint/eslint-plugin-template": "19.1.0",
    "@angular-eslint/schematics": "19.1.0",
    "@angular-eslint/template-parser": "19.1.0",
    "@angular/cli": "^19.1.7",
    "@angular/compiler-cli": "^19.1.6",
    "@types/node": "^18.19.76",
    "@types/office-js": "^1.0.467",
    "@types/office-runtime": "^1.0.35",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^8.53.0",
    "eslint-config-office-addins": "^1.7.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^46.9.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "office-addin-debugging": "^6.0.3",
    "office-addin-dev-certs": "^2.0.3",
    "office-addin-manifest": "^2.0.3",
    "ts-node": "~10.9.1",
    "typescript": "~5.7.3"
  }
}
