{
  "name": "spellchecker-addin-office",
  "version": "1.2.0",
  "scripts": {
    "build": "ng build --configuration production --base-href=/addin/",
    "build:test": "ng build --configuration test",
    "build:dev": "ng build",
    "dev-server": "bash -c \"ng serve --ssl --ssl-key ~/.office-addin-dev-certs/localhost.key --ssl-cert ~/.office-addin-dev-certs/localhost.crt\"",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "ng": "ng",
    "ssl:config": "office-addin-dev-certs install",
    "start:desktop": "office-addin-debugging start ./src/manifest.xml desktop",
    "start:desktop:prod": "office-addin-debugging start ./src/manifest-prod.xml desktop",
    "start:web": "office-addin-debugging start ./src/manifest.xml web",
    "start:web:prod": "office-addin-debugging start ./src/manifest-prod.xml web",
    "stop": "office-addin-debugging stop ./src/manifest.xml",
    "stop:prod": "office-addin-debugging stop ./src/manifest-prod.xml",
    "test": "ng test",
    "test:coverage": "ng test --watch false --code-coverage",
    "validate": "office-toolbox validate -m ./src/manifest.xml",
    "validate:prod": "office-toolbox validate -m ./src/manifest-prod.xml"
  },
  "license": "MIT",
  "config": {
    "app-to-debug": "word",
    "app-type-to-debug": "desktop",
    "dev-server-port": 4200
  },
  "dependencies": {
    "@angular/animations": "^17.0.7",
    "@angular/common": "^17.0.7",
    "@angular/compiler": "^17.0.7",
    "@angular/core": "^17.0.7",
    "@angular/forms": "^17.0.7",
    "@angular/platform-browser": "^17.0.7",
    "@angular/platform-browser-dynamic": "^17.0.7",
    "@angular/router": "^17.0.7",
    "@farscrl/rumantsch-language-tools": "^1.0.1",
    "@iharbeck/ngx-virtual-scroller": "17.0.0",
    "@microsoft/office-js-helpers": "^1.0.2",
    "@ngneat/dialog": "^4.0.0",
    "ngx-matomo-client": "^6.0.0",
    "core-js": "^3.29.1",
    "ngx-spinner": "^16.0.2",
    "ngx-toastr": "^17.0.2",
    "office-ui-fabric-js": "^1.5.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1700.7",
    "@angular-devkit/build-angular": "^17.0.7",
    "@angular-eslint/builder": "17.1.1",
    "@angular-eslint/eslint-plugin": "17.1.1",
    "@angular-eslint/eslint-plugin-template": "17.1.1",
    "@angular-eslint/schematics": "17.1.1",
    "@angular-eslint/template-parser": "17.1.1",
    "@angular/cli": "^17.0.7",
    "@angular/compiler-cli": "^17.0.7",
    "@types/node": "^18.15.3",
    "@types/office-js": "^1.0.318",
    "@types/office-runtime": "^1.0.29",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-office-addins": "^1.6.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "40.0.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "office-addin-debugging": "^5.0.4",
    "office-addin-dev-certs": "^1.11.2",
    "office-toolbox": "^0.3.0",
    "ts-node": "~10.9.1",
    "typescript": "~5.2.2"
  }
}
