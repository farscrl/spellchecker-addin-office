{
  "name": "spellchecker-addin-office",
  "version": "1.3.8",
  "scripts": {
    "build": "node scripts/generate-version.js && ng build --configuration production --base-href=/addin/",
    "build:staging": "node scripts/generate-version.js && ng build --configuration test  --base-href=/addin/",
    "build:dev": "node scripts/generate-version.js && ng build",
    "dev-server": "bash -c \"node scripts/generate-version.js && ng serve --ssl --ssl-key ~/.office-addin-dev-certs/localhost.key --ssl-cert ~/.office-addin-dev-certs/localhost.crt\"",
    "postinstall": "node scripts/generate-version.js",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "ng": "ng",
    "ssl:config": "office-addin-dev-certs install",
    "start:desktop": "office-addin-debugging start ./src/manifest.xml desktop",
    "start:desktop:prod": "office-addin-debugging start ./src/manifest-prod.xml desktop",
    "start:web": "office-addin-debugging start ./src/manifest.xml web",
    "start:web:prod": "office-addin-debugging start ./src/manifest-prod.xml web",
    "stop": "office-addin-debugging stop ./src/manifest.xml",
    "stop:prod": "office-addin-debugging stop ./src/manifest-prod.xml",
    "test": "ng test",
    "test:coverage": "ng test --watch false --code-coverage",
    "validate": "office-addin-manifest validate ./src/manifest.xml",
    "validate:staging": "office-addin-manifest validate ./src/manifest-staging.xml",
    "validate:prod": "office-addin-manifest validate ./src/manifest-prod.xml"
  },
  "license": "MIT",
  "config": {
    "app-to-debug": "word",
    "app-type-to-debug": "desktop",
    "dev-server-port": 4200
  },
  "dependencies": {
    "@angular/animations": "^21.1.5",
    "@angular/common": "^21.1.5",
    "@angular/compiler": "^21.1.5",
    "@angular/core": "^21.1.5",
    "@angular/forms": "^21.1.5",
    "@angular/platform-browser": "^21.1.5",
    "@angular/platform-browser-dynamic": "^21.1.5",
    "@angular/router": "^21.1.5",
    "@farscrl/rumantsch-language-tools": "^1.0.4",
    "@iharbeck/ngx-virtual-scroller": "^20.0.0",
    "@microsoft/office-js-helpers": "^1.0.2",
    "@ngneat/dialog": "^5.2.1",
    "core-js": "^3.48.0",
    "ngx-matomo-client": "^9.0.1",
    "ngx-spinner": "^21.0.0",
    "ngx-toastr": "^20.0.5",
    "office-ui-fabric-js": "^1.5.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.2101.4",
    "@angular-devkit/build-angular": "^21.1.4",
    "@angular-eslint/builder": "21.2.0",
    "@angular-eslint/eslint-plugin": "21.2.0",
    "@angular-eslint/eslint-plugin-template": "21.2.0",
    "@angular-eslint/schematics": "21.2.0",
    "@angular-eslint/template-parser": "21.2.0",
    "@angular/cli": "^21.1.4",
    "@angular/compiler-cli": "^21.1.5",
    "@types/node": "^22.19.1",
    "@types/office-js": "^1.0.574",
    "@types/office-runtime": "^1.0.35",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-config-office-addins": "^2.0.6",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^61.2.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "office-addin-debugging": "^6.0.3",
    "office-addin-dev-certs": "^2.0.3",
    "office-addin-manifest": "^2.0.3",
    "ts-node": "~10.9.1",
    "typescript": "~5.9.3"
  },
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184"
}
