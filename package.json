{
  "name": "office-angular",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build --configuration production",
    "build:dev": "ng build",
    "dev-server": "bash -c \"ng serve --ssl --ssl-key ~/.office-addin-dev-certs/localhost.key --ssl-cert ~/.office-addin-dev-certs/localhost.crt\"",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "ng": "ng",
    "ssl:config": "office-addin-dev-certs install",
    "start:desktop": "office-addin-debugging start ./src/manifest.xml desktop",
    "start:web": "office-addin-debugging start ./src/manifest.xml web",
    "stop": "office-addin-debugging stop ./src/manifest.xml",
    "test": "ng test",
    "test:coverage": "ng test --watch false --code-coverage",
    "validate": "office-toolbox validate -m ./src/manifest.xml"
  },
  "private": true,
  "license": "MIT",
  "config": {
    "app-to-debug": "word",
    "app-type-to-debug": "desktop",
    "dev-server-port": 4200
  },
  "dependencies": {
    "@angular/animations": "~11.2.3",
    "@angular/common": "~11.2.3",
    "@angular/compiler": "~11.2.3",
    "@angular/core": "~11.2.3",
    "@angular/forms": "~11.2.3",
    "@angular/platform-browser": "~11.2.3",
    "@angular/platform-browser-dynamic": "~11.2.3",
    "@angular/router": "~11.2.3",
    "@microsoft/office-js-helpers": "^1.0.2",
    "core-js": "^2.6.12",
    "office-ui-fabric-js": "^1.5.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1102.2",
    "@angular-devkit/build-angular": "~0.1102.2",
    "@angular-eslint/builder": "1.1.0",
    "@angular-eslint/eslint-plugin": "1.1.0",
    "@angular-eslint/eslint-plugin-template": "1.1.0",
    "@angular-eslint/schematics": "1.1.0",
    "@angular-eslint/template-parser": "1.1.0",
    "@angular/cli": "~11.2.2",
    "@angular/compiler-cli": "~11.2.3",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/office-js": "^1.0.166",
    "@types/office-runtime": "^1.0.17",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "eslint": "^7.6.0",
    "eslint-config-office-addins": "^1.0.20",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "office-addin-debugging": "^4.0.3",
    "office-addin-dev-certs": "^1.5.12",
    "office-toolbox": "^0.3.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.1.2"
  }
}
